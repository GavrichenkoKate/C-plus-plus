#include <iostream>

using namespace std;
int main() {
    int n, i, j;
    cout << "Enter n: ";
    cin >> n;
    for (i = 1; i <= n; i++) {
        for (j = 2; j <= i / 2; j++) {
            if (i % j == 0) break;
        }
        if (j > i / 2) cout << i << " ";
    }
    cout << endl;
 system("pause");
 return 0;
}

